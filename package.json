{
	"name": "obsidian-tasks-kanban",
	"version": "1.0.0",
	"description": "Kanban board view for Obsidian Tasks plugin. Visualize and manage your tasks in a drag-and-drop kanban interface.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "node esbuild.config.mjs production",
		"build-with-check": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint ./src --fix && tsc --noEmit --pretty"
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin",
		"tasks",
		"kanban",
		"task-management"
	],
	"author": "Tasks Kanban Team",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"async-mutex": "^0.5.0",
		"boon-js": "^2.0.5",
		"chrono-node": "2.3.9",
		"eventemitter2": "^6.4.9",
		"flatpickr": "^4.6.13",
		"i18next": "^21.10.0",
		"moment": "^2.29.4",
		"mustache": "^4.2.0",
		"mustache-validator": "^0.2.0",
		"rrule": "^2.8.1"
	}
}
